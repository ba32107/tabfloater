{
  "name": "TabFloater",
  "version": "0.4",
  "description": "Picture-in-Picture for ANY tab!",
  "permissions": [
    "activeTab",
    "nativeMessaging",
    "notifications",
    "storage",
    "tabs"
  ],
  "key": "",
  "background": {
    "page": "html/background.html",
    "persistent": false
  },
  "browser_action": {
    "default_popup": "html/popup.html",
    "default_title": "TabFloater",
    "default_icon": {
      "16": "images/icons/16.png",
      "32": "images/icons/32.png",
      "48": "images/icons/48.png",
      "128": "images/icons/128.png"
    }
  },
  "options_ui": {
    "page": "html/options.html",
    "open_in_tab": false
  },
  "commands": {
    "floatTab": {
      "suggested_key": {
        "default": "Alt+PageDown"
      },
      "description": "Float tab"
    },
    "unfloatTab": {
      "suggested_key": {
        "default": "Alt+PageUp"
      },
      "description": "Unfloat tab"
    }
  },
  "icons": {
    "16": "images/icons/16.png",
    "32": "images/icons/32.png",
    "48": "images/icons/48.png",
    "128": "images/icons/128.png"
  },
  "manifest_version": 2
}